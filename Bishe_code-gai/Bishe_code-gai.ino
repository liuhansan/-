void setup() 
{
  pinMode(13,OUTPUT);//设置13号引脚为输出模式
  Serial.begin(9600);//设置波特率9600
  BTserial.begin(9600);//蓝牙模块波特率设置为9600
  while(!Serial);//等待串口即舵机控制板通信成功再往下执行程序
  digitalWrite(13,HIGH);//arduino的LED亮起
  pinMode(5,OUTPUT);
  pinMode(4,INPUT);//设置超声波引脚
  LEDS.addLeds<LED_TYPE, LED_DT, COLOR_ORDER>(leds, NUM_LEDS);  // 初始化光环
  FastLED.setBrightness(max_bright);                           // 设置光环灯珠亮度
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // 将 I2C 地址初始化为 0x3C（用于 128x64 显示器）
  display.clearDisplay(); // 在启动程序之前清除显示以避免adafruit闪屏
  display.drawBitmap(0, 0, bitmap_g1ldrm, 128, 64, WHITE);//加载图片‘欢迎使用’
  display.display();
  delay(1000);
  display.clearDisplay(); // 在启动程序之前清除显示以避免adafruit闪屏
  display.drawBitmap(0, 0, bitmap_c9e2yn, 128, 64, WHITE);//加载图片‘I'm MIMIC’
  display.display();
  delay(2000);
}

void loop() { 
  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  duration = pulseIn(echoPin, HIGH);
  distanceCm = duration * 0.034 / 2;//开启超声波测距并且获得距离
  if( BTserial.available())
  {
    char BTCMD = BTserial.read();//将软件串口中的数据赋值给变量BTCMD
    if (Mode == 1)//判断指令并且进入手动控制模式
    {controlMode(BTCMD);
    delay(50);
    }
    else {
    AutoModswitch(BTCMD);
    delay(50);
    }
  }
if(Mode == 0)
{ FastLED.setBrightness(max_bright);
  float Nowdistance = distanceCm;
  if (Nowdistance < mindistance)
    {
    fill_solid(leds, 8, CRGB::Yellow);
    FastLED.show();
    display.clearDisplay();
    display.drawBitmap(0, 0, bitmap_6x51mr, 128, 64, WHITE);
    display.display();//加载图片‘避障中。。。’
    delay(40);
    myse.stopActionGroup();
    myse.runActionGroup(4,1);
    delay(1000);
    }//若距离大于预定则持续往前走
    else
    {
    fill_solid(leds, 8, CRGB::Snow);
    FastLED.show();
    display.clearDisplay();
    display.drawBitmap(0, 0, bitmap_ddsmc, 128, 64, WHITE);
    display.display();//加载图片‘前进中...’
    delay(40);
    myse.stopActionGroup();
    myse.runActionGroup(8,1);
    delay(1000);
    }//若小于预定值则左转转向
}
else 
{ if(StMode == 1)
  {NoactionMode();
}
else
{
  JuststayMode();
}
}
