void controlMode(char BTCMD)//手动控制模式函数
{ 
  display.clearDisplay();
  display.drawBitmap(0, 0, bitmap_8ka9jq, 128, 64, WHITE);
  display.display();//加载图片‘控制模式’
  switch(BTCMD)
  {
  case 'w':  // 前进
  myse.stopActionGroup();
  myse.runActionGroup(8,1);
  delay(500);
  break;
  case 'a':
  myse.stopActionGroup();
  myse.runActionGroup(4,1);//执行左转指令一次
  delay(500);
  break;
  case 'd':
  myse.stopActionGroup();
  myse.runActionGroup(6,1);//执行右转指令一次
  delay(500);
  break;
  case 'q':
  myse.stopActionGroup();
  myse.runActionGroup(7,1);//执行左挥手指令一次
  delay(500);
  break;
  case 'e':
  myse.stopActionGroup();
  myse.runActionGroup(9,1);//执行右挥手指令一次
  delay(500);
  break;
  case 'f':
  myse.stopActionGroup();
  myse.runActionGroup(88,10);//开启不断前进模式
  delay(500);
  break;
  case 's':
  myse.stopActionGroup();//停止不断前进
  display.clearDisplay(); // 在启动程序之前清除显示以避免adafruit闪屏
  display.drawBitmap(0, 0, bitmap_gt0ik, 128, 64, WHITE);//加载图片‘思考中’
  display.display();
  delay(5000);
  break;
  case 'j':
  StMode = 0;
  break;
  case 'J':
  StMode = 1;
  break;
  case 'A':
  Mode = 0;//切换为自主模式
  delay(500);
  default:
  break;
}
